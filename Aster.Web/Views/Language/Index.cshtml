@model Aster.Web.Models.LanguageListModel
@{
    ViewData["Title"] = "Index";
}
<h2>Languages</h2>


<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Culture</th>
            <th>Rtl</th>
            <th>Enabled</th>
            <th>Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var language in Model.LanguageList) {
        <tr>
            <td>@language.Id</td>
            <td>@language.Name</td>
            <td>@language.LanguageCulture</td>
            <td>@language.Rtl</td>
            <td>@language.Enabled</td>
            <td>@language.Orders</td>
        </tr>
        }
    </tbody>
</table>


<pre lang="xml">@Model.tempXml</pre>

<pre>@Model.tempJson</pre>

